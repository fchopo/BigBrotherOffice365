@model string
<h2>Stop Subscription</h2>

<form method="post" class="form-horizontal">
    <div class="form-group">
        <label>Content Type: @Model</label>
        <input type="hidden" name="id" value="@Model"/>
    </div>
    <div class="form-group">
        <button type="submit" asp-page-handler="Stop" class="btn btn-danger">
            Proceed
        </button>
    </div>
</form>

